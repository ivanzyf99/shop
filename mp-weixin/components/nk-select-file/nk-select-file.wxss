.file-outerBox {
  width: 100%;
  height: 100vh;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 1070;
  padding-bottom: 40rpx;
  background-color: #fff;
}
.file-outerBox .file-titel {
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  padding: 0 32rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.file-outerBox .file-titel .file-nav-leftBox {
  width: 60rpx;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}
.file-outerBox .file-titel .file-nav-leftBox .file-back-img {
  width: 36rpx;
}
.file-outerBox .file-titel .file-nav-rightBox {
  width: 60rpx;
  height: 100%;
}
.file-outerBox .file-address {
  width: 100%;
  height: 60rpx;
  background-color: #FBFBFB;
  padding: 0 32rpx;
  display: flex;
  color: #373737;
  font-size: 24rpx;
}
.file-outerBox .file-address .address-scroll {
  width: calc(100% - 116rpx);
  white-space: nowrap;
  height: 100%;
}
.file-outerBox .file-address .address-scroll .address-box {
  height: 60rpx;
  line-height: 60rpx;
  display: inline-block;
}
.file-outerBox .file-address .address-scroll .address-box .to-img-box {
  width: 20rpx;
}
.file-outerBox .file-address .root-box {
  width: 116rpx;
  height: 60rpx;
  line-height: 60rpx;
  display: inline-block;
  box-shadow: 10rpx 0 10rpx -10rpx rgba(8, 8, 8, 0.3);
}
.file-outerBox .file-address .root-box .to-img-box {
  width: 20rpx;
}
.file-outerBox .folder-box {
  height: 120rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin: 0 32rpx;
  border-bottom: 1px solid #EEEEEE;
}
.file-outerBox .folder-box .folder-name-box {
  width: 80%;
  display: flex;
  align-items: center;
  flex-wrap: wrap;
}
.file-outerBox .folder-box .folder-name-box .folder-img {
  width: 72rpx;
  margin-right: 16rpx;
}
.file-outerBox .folder-box .folder-name-box .name-box {
  width: calc(100% - 100rpx);
  overflow: hidden;
  word-wrap: break-word;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.file-outerBox .folder-box .toFolder-img {
  width: 28rpx;
}
.file-outerBox .select-tips {
  width: 100%;
  height: 160rpx;
  text-align: center;
  font-size: 32rpx;
  color: #888;
  padding-top: 60rpx;
}
.file-outerBox .file-box {
  height: 120rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin: 0 32rpx;
  border-bottom: 1px solid #EEEEEE;
}
.file-outerBox .file-box .file-name-box {
  width: 80%;
  display: flex;
  align-items: center;
}
.file-outerBox .file-box .file-name-box .file-img {
  width: 72rpx;
  margin-right: 16rpx;
}
.file-outerBox .file-box .file-name-box .name-box {
  width: calc(100% - 100rpx);
  overflow: hidden;
  word-wrap: break-word;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.file-outerBox .file-box .select-img {
  width: 32rpx;
}
.file-outerBox .select-foot-btn {
  width: calc(100% - 64rpx);
  margin-top: 20rpx;
}

